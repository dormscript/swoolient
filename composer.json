{
    "name": "meanevo/swoolient",
    "description": "A swoole clients manager in multiple processes",
    "type": "library",
    "keywords": ["client", "socket", "swoole"],
    "license": "MIT",
    "authors": [
        { "name": "meanevo", "email": "admin@meanevo.com" }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-swoole": "^4",
        "vlucas/phpdotenv": "^2.4",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "suggest": {
        "bramus/monolog-colored-line-formatter": "Colorize logs printing to console",
        "sentry/sentry": "Add error reporting support with sentry"
    },
    "autoload": {
        "psr-4": {
            "MeanEVO\\Swoolient\\": "src/"
        },
        "files": [
            "src/Helpers/global.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
