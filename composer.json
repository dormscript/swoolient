{
    "name": "meanevo/swoolient",
    "description": "A swoole manager for clients in multiple processes",
    "keywords": [
        "client",
        "socket",
        "swoole"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "meanevo",
            "email": "admin@meanevo.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-swoole": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "monolog/monolog": "^1.23"
    },
    "suggest": {
        "bramus/monolog-colored-line-formatter": "Colorize loggings printing to console",
        "sentry/sentry": "Add error reporting functionality with sentry"
    },
    "autoload": {
        "psr-4": {
            "MeanEVO\\Swoolient\\": "src/"
        },
        "files": [
            "src/Helpers/global.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
